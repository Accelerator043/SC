hist(x3, breaks = x0, ylim = c(0, 250), main = "sigma = 2.0")
lines(x0, rld(x0, 2), lty = 2, col = 2)
#sigma=3
x4 <- rayleigh(3, 1000)
hist(x4, breaks = x0, ylim = c(0, 250), main = "sigma = 3.0")
lines(x0, rld(x0, 3), lty = 2, col = 2)
